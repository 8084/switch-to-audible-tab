{
    "manifest_version": 2,
    "name": "Switch to audible tab",
    "version": "0.0.1",
    "description": "Switch to tab that is currently making sound.",
    "icons": {
        "128": "img/icon.png"
    },
    "background": {
        "scripts": ["src/background.js"]
    },
    "browser_action": {
        "default_icon": {
            "128": "img/icon.png"
        },
        "default_title": "Switch to audible tab (Alt+Shift+A)"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+A"
            }
        }
    },
    "permissions": [
        "tabs", "storage", "windows"
    ]
}
